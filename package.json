{
  "name": "metrytics-client",
  "version": "1.0.0",
  "description": "A simple npm package for sending analytics data.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:clear": "jest --clearCache && jest",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pumbi/metrytics-client.git"
  },
  "keywords": [
    "analytics",
    "tracking",
    "npm",
    "typescript",
    "javascript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@jest/types": "^29.6.3",
    "@types/node": "^22.13.10",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.1.0",
    "typescript": "^4.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/test/**/*.test.ts"
    ],
    "setupFiles": [
      "./test/setup.ts"
    ]
  }
}
